    "mongodb-memory-server": "^9.1.1",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/__tests__/**"
    ]
  }
